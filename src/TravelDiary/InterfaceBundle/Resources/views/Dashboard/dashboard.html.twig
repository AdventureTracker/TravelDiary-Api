{% extends "@TravelDiaryInterface/_layout/base.html.twig" %}

{% block title %}Dashboard{% endblock %}

{% block content %}

	<div class="row">
		<!-- Profile -->
		<div class="col-lg-4">

			<div class="panel panel-default">
				<div class="panel-heading">
					<h3 class="panel-title">{{ app.user.getFullName() }}</h3>
				</div>
				<div class="panel-body">
					<img src="{{ app.user.usrEmail|gravatar('500') }}" class="img-responsive">
				</div>
				<div class="panel-footer">
					<table style="width: 100%">
						<tr>
							<td title="Devices"><i class="material-icons left">stay_current_portrait</i> 0</td>
							<td title="Trips"><i class="material-icons left">language</i> 0</td>
							<td title="Trip records"><i class="material-icons left">location_on</i> 0</td>
						</tr>
					</table>
				</div>
			</div>
		</div>

		<div class="col-lg-8">

		</div>
	</div>

{% endblock %}
